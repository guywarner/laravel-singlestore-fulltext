{
    "name": "guywarner/laravel-singlestore-fulltext",
    "description": "Package to use SingleStore DB's match() function for full text search adding whereMatch() to Models",
    "keywords": [
        "guywarner",
        "laravel",
        "laravel-singlestore-fulltext"
    ],
    "homepage": "https://github.com/guywarner/laravel-singlestore-fulltext",
    "license": "MIT",
    "authors": [
        {
            "name": "Guy Warner",
            "email": "gwarner@monitorbase.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Guywarner\\LaravelSinglestoreFulltext\\": "src",
            "Guywarner\\LaravelSinglestoreFulltext\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guywarner\\LaravelSinglestoreFulltext\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guywarner\\LaravelSinglestoreFulltext\\LaravelSinglestoreFulltextServiceProvider"
            ],
            "aliases": {
                "LaravelSinglestoreFulltext": "Guywarner\\LaravelSinglestoreFulltext\\Facades\\LaravelSinglestoreFulltext"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
